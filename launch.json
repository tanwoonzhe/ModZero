{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ModZero API (Uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "backend.app.main:asgi_app",
        "--reload",
        "--port",
        "8000"
      ],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}\\.env",
      "console": "integratedTerminal"
    }
  ]
}
